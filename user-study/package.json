{
  "name": "user-study",
  "version": "1.0.0",
  "description": "API Server for User Study",
  "repository": "https://github.com/sardankur/dark-patterns.git",
  "main": "server.js",
  "scripts": {
    "sequelize": "sequelize --options-path='./sequelizerc.js'",
    "lint": "eslint --ext .js api tests config",
    "lint:fix": "eslint --ext .js api --fix",
    "clean": "rimraf public && rimraf compiled",
    "start": "npm run build && node compiled/server.js",
    "load_users": "babel-node services/tasks/load_users.js",
    "dev": "cross-env NODE_ENV=development nodemon",
    "build:dev": "cross-env NODE_ENV=development webpack --debug --colors --display-error-details --config ./webpack/webpack.config.js",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --colors --display-error-details --config ./webpack/webpack.config.js",
    "cov": "NODE_ENV=test PORT=3002 ./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha --dir test/coverage ./test/setup.js ./test/**/*.js",
    "test": "NODE_ENV=test PORT=3002 mocha ./test/setup.js ./test/**/*.js",
    "test:watch": "NODE_ENV=test PORT=3002 mocha ./test/setup.js --watch ./test/**/*.js",
    "tasks:run": "babel-node services/tasks/index.js"
  },
  "author": "Souranil Sen",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^7.2.1",
    "enzyme": "^2.8.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "expect": "^1.15.2",
    "istanbul": "1.1.0-alpha.1",
    "jsdom": "^9.12.0",
    "mocha": "^3.4.2",
    "node-babel": "^0.1.2",
    "nodemon": "^1.10.2",
    "pre-commit": "^1.2.2",
    "sinon": "^2.3.2"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "sourceMaps": true,
    "retainLines": true
  },
  "engines": {
    "node": ">=6.x"
  },
  "dependencies": {
    "autoprefixer": "^6.7.7",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.0",
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.1",
    "chai": "^4.0.0",
    "classnames": "^2.2.3",
    "compression": "^1.6.2",
    "connect-pg-simple": "^3.1.0",
    "cors": "^2.8.3",
    "cross-env": "^4.0.0",
    "crypto": "^0.0.3",
    "es6-promise": "^4.1.0",
    "express": "^4.15.2",
    "express-flash": "0.0.2",
    "express-session": "^1.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "global": "^4.3.2",
    "helmet": "^3.5.0",
    "json-loader": "^0.5.4",
    "jsonwebtoken": "^7.4.1",
    "lodash": "^4.17.4",
    "method-override": "^2.3.8",
    "moment-range": "^3.0.3",
    "mustbe": "^0.4.0",
    "nodemailer": "^4.0.1",
    "passport": "^0.3.2",
    "passport-google-oauth": "^1.0.0",
    "passport-jwt": "^2.2.1",
    "passport-local": "^1.0.0",
    "pg": "^6.1.5",
    "pg-hstore": "^2.3.2",
    "postcss-cssnext": "^2.10.0",
    "postcss-import": "^9.0.0",
    "postcss-loader": "^1.3.3",
    "postcss-reporter": "^3.0.0",
    "rimraf": "^2.6.1",
    "sequelize": "^3.30.4",
    "sequelize-cli": "^2.7.0",
    "sequelize-fixtures": "^0.5.6",
    "sequelize-log-syntax-colors": "^1.0.0",
    "spark-md5": "^3.0.0",
    "style-loader": "^0.16.1",
    "superagent": "^3.5.2",
    "supertest": "^3.0.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.3.3",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.18.0",
    "winston": "^2.3.1"
  }
}
